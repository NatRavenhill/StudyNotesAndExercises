//example of a list
var list = {
    value: 1
    rest: {
	value: 2
        rest: {
	    value: 3
	    rest: null
	}
    }
}

//converts a list to an array
function listToArray(list){
    var arr = [];
    while(list.value !== null){
	arr.push(list.value);
	list = list.rest;
    }
    return arr;
}

//converts an array to a list
